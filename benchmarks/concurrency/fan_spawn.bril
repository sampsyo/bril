@id(x:int): int { ret x; }

@main(): void {
  one:int = const 1;
  two:int = const 2;
  three:int = const 3;
  four:int = const 4;

  t1:thread = spawn @id one;
  t2:thread = spawn @id two;
  t3:thread = spawn @id three;
  t4:thread = spawn @id four;

  join t1; join t2; join t3; join t4;
  z:int = const 0;
  print z;
  ret;
}